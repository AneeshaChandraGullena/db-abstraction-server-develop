    },
    "certs": {
      "base_path": "/kp_data/config",
      "ca_cert_pem": "ca-cert.pem",
      "client_cert_pem": "client-cert.pem",
      "client_key_pem": "client-key.pem",
      "server_name": "MariaDB server"
    }
