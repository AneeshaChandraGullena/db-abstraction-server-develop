{
    "csvDir": "kp-csv/",
    "host": {
        "name": "DB abstraction server",
        "ipv4_address": "127.0.0.1",
        "port": 8985
    },
    "openstack": {
        "barbican": {
            "url": "https://localhost:9311"
        }
    },
    "database":{
      "credentialsLocation" : "/opt/keyprotect/key-management-api/config/keyprotect_db.json",
      "table" : "keyprotect_ids"
    },
    "version": {
        "semver": "",
        "commit": "",
        "runtime": ""
    },
    "timeouts":{
      "grpcTimeout" : 30
    },
    "keyManager": {
        "name": "Key Manager gRPC service",
        "ipv4_address": "127.0.0.1",
        "port": 8942
    },
    "feature_toggles": {
      "encrypt_metadata": false,
      "encrypt_user_metadata": false,
      "orders" : true,
      "enableTLS": false
